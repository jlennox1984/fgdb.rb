<h1>Editing meeting</h1>

<%= form_tag :action => 'update', :id => @meeting do %>
  <%= render :partial => 'form' %>
  <div class="follow_scroll right_side">
    <%= submit_tag 'Edit' %><br  />
    <%= link_to 'Show', :action => 'show', :id => @meeting %> |
    <%= link_to 'Back', :action => 'list' %>
  </div>
<% end %>

