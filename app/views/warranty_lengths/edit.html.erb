<h1>Editing warranty length</h1>

<%= form_for(@warranty_length, :url => {:action => "update", :id => @warranty_length.id}) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => "form", :locals => {:f => f} %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', {:action => "index"} %>
